#!/bin/bash
SEL_DIR='temp/sv-selenium'

SELENIUM_URL='http://selenium.googlecode.com/files/selenium-server-standalone-2.28.0.jar'

echo $SELENIUM_URL >> $SEL_DIR/selenium_env
cat $SEL_DIR/selenium_env

mkdir -p $SEL_DIR
rm -rf $SEL_DIR/selenium-server*
cd $SEL_DIR

wget $SELENIUM_URL 

mv selenium-server-standalone*.jar selenium-server-standalone.jar

